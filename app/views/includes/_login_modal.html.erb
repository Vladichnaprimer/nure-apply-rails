<div aria-labelledby="myModalLabel" class="modal fade" id="authorization_modal" role="dialog" tabindex="-1">  <div class="modal-dialog" role="document">    <div class="modal-content">      <button aria-label="Close" class="close" data-dismiss="modal" type="button">        <div class="close_wrap">          <span class="close_elem"></span>          <span class="close_elem"></span>        </div>      </button>      <%= link_to new_user_registration_path do %>          <div class="modal-header">            <div class="top_thumbnail">              <%= image_tag 'modal_top.svg', class: 'img-responsive' %>            </div>            <h4 class="modal-title" id="myModalLabel"></h4>          </div>      <% end %>      <div class="modal-body">        <%= render partial: "devise/sessions/form" %>      </div>    </div>  </div></div>